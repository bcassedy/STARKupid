<div class='row'>
  <div class='col-xs-10 col-xs-offset-1'>
    <h1>Questions</h1>
    <% if (questions.length === 0) { %>
      <p class='text-center'>Come Back Later For More Questions</p>
    <% } %>
    <% questions.each(function (question) { %>
      <div class='row'>
        <a href='#/questions/<%= question.id %>'>
          <%= question.escape('title') %>
        </a>
      </div>
    <% }); %>
  </div>
</div>