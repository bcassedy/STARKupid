<div class='row'>
  <h1>Questions</h1>
  <div class='col-xs-10 col-xs-offset-2'>
    <% if (questions.length === 0) { %>
      <%= 'Come Back Later For More Questions' %>
    <% } %>
    <% questions.each(function (question) { %>
      <div class='row'>
        <a href='#/questions/<%= question.id %>'>
          <%= question.escape('title') %>
        </a>
      </div>
    <% }); %>
  </div>
</div>