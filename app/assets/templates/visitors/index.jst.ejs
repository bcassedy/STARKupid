<div class='row'>
  <div class='col-xs-10'>
    <h1>Visitors</h1>
  </div>
  <div class='col-xs-10'>
      <% visitors.each(function (profile) { %>
        <a href='#/profiles/<%= profile.escape("username") %>'><div class='col-xs-3 profile-div'>
          <div class='index-photo-div'>
            <img
              src="<%= profile.escape('photo_url') %>"
              alt='Photo'
              class='index-photo'>
          </div>
          <p><%= profile.escape('username') %></p>
          <p><%= profile.escape('age') + ' | ' + profile.escape('location') %></p>
          <p><%= profile.escape('match_percentage') %> % Match</p>
        </div>
        </a>
      <% }); %>
  </div>
</div>