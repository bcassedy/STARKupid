<h1>Matches</h1>
<div class='panel'>
  <div class='col-xs-8 col-xs-offset-2'>
    <% @profiles.each do |profile| %>
      <div class='col-xs-4 panel panel-default'>
        <div class='index-photo'></div>
        <p><%= profile.username %></p>
        <p><%= profile.age.to_s + ' | ' + profile.location %></p>
      </div>
    <% end %>
  </div>
</div>
